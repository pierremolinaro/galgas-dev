#include "version_libpm.h"
#if LIBPM_VERSION != THE_LIBPM_VERSION
  #error "This file has been compiled with a version of GALGAS different than the version of libpm"
#endif

#include <string.h>

//---------------------------------------------------------------------------*

#include "%!OPTION_COMPONENT_NAME%.h"

//---------------------------------------------------------------------------*
//                                                                           *
//                               Bool options                                *
//                                                                           *
//---------------------------------------------------------------------------*

%foreach BOOL_OPTION_MAP do
  %C_BoolCommandLineOption gOption_%![OPTION_COMPONENT_NAME identifierRepresentation]%_%![key identifierRepresentation]% ("%!OPTION_COMPONENT_NAME%",\n%
  %                                         %![key utf8Representation]%,\n%
  %                                         %![[[mOptionChar char] uint] string]%,\n%
  %                                         %![mOptionString utf8Representation]%,\n%
  %                                         %![mComment utf8Representation]%) ;\n\n%
end foreach
