%foreach STRING_OPTION_MAP do
  %C_StringCommandLineOption gOption_%![OPTION_COMPONENT_NAME identifierRepresentation]%_%![key identifierRepresentation]% ("%!OPTION_COMPONENT_NAME%",\n%
  %                                         %![key utf8Representation]%,\n%
  %                                         %![[[mOptionChar char] uint] string]%,\n%
