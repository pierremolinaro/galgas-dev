extern @complex {
  "// No Predeclaration\n"
}{
  "  private : bool mIsValid ;\n"
  "  private : double mReal ;\n"
  "  private : double mImaginary ;\n"
}{
  constructor new ??@double real ??@double imaginary -> @complex
  getter real -> @double
  getter imaginary -> @double
}

