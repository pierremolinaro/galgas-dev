

#-----------------------------------------------------------------------------

uniquemap @maTable {
  @uint mAttribut
  insert insertKey error message "internal error"
}

mapproxy @monMapProxy (@maTable)

#-----------------------------------------------------------------------------

proc testsuite_mapproxy {
message "*************************************** begin of maproxy test\n"
  var table = [@maTable emptyMap]
  @monMapProxy proxy
  [@monMapProxy makeProxyFromString !?table !"abc" ?proxy]
  log "[table count]": [table count]
  log "Proxy: " : proxy
  log "[table unsolvedProxyCount]": [table unsolvedProxyCount]
}

