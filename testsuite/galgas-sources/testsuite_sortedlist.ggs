

sortedlist @mySortedList {
  public let @string mValue
}{
  mValue >
}

proc testsuite_sortedlist() {
  var strList = @mySortedList []
  strList += !"b"
  strList += !"a"
  strList += !"d"
  strList += !"c"
  println ("*************************************** begin of sorted list test")
  for s in strList do (idx)
    println ("  index " + idx + " : " + s.mValue)
  end
}

