semantics testsuite_sortedlist :

sortedlist @mySortedList {
  @string mValue ;
}{
  mValue >
}

routine testsuite_sortedlist :
  @mySortedList strList [emptySortedList] ;
  strList += !"b" ;
  strList += !"a" ;
  strList += !"d" ;
  strList += !"c" ;
  message "*************************************** begin of sorted list test\n" ;
  for s in strList do index idx
    message "  index " . idx . " : " . s->mValue . "\n" ;
  end for ;
end routine ;

end semantics ;
