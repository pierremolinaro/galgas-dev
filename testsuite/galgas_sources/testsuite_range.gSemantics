semantics testsuite_range :

routine testsuite_range :
  message "*************************************** begin of range test\n" ;
  @string s := "Range [4, 3] enumeration:" ;
  for i in [@range new !4 ! 3] do
    s.= " " . i ;
  end for ;
  log s ;
  s := "Range [4, 3] reverse enumeration:" ;
  for i in reverse [@range new !4 ! 3] do
    s.= " " . i ;
  end for ;
  log s ;
end routine ;


end semantics ;
