#----------------------------------------------------------------------------*#                                                                            *#  This file has been used for testing LR(1) computations.                   *#                                                                            *# From the book "COMPILATEURS, principes, techniques et outils", by Alfred   *# Aho, Ravi Sethi, Jeffrey Ullman, page 260, exemple 4.42.                   *#                                                                            *#----------------------------------------------------------------------------*#----------------------------------------------------------------------------*#  S C A N N E R                                                             *#----------------------------------------------------------------------------*lexique test_LR1_scanner :#--- Key words$c$ error message "'c' keyword" ;$d$ error message "'d' keyword" ;rule 'c' :  send $c$ ;end rule ;rule 'd' :  send $d$ ;end rule ;# --------------------- separatorsrule '\1' -> ' ' :end rule ;end lexique ;#----------------------------------------------------------------------------*#  P A R S E R                                                               *#----------------------------------------------------------------------------*syntax test_LR1_expressionParser :import lexique test_LR1_scanner ;rule <S> ;rule <C> ;rule <S> : <C> ; <C> ;end rule ;rule <C> : $c$ ; <C> ;end rule ;rule <C> : $d$ ;end rule ;end syntax ;#----------------------------------------------------------------------------*#  G R A M M A R                                                             *#----------------------------------------------------------------------------*grammar test_LR1_grammar "LR1" :  import lexique test_LR1_scanner ;  import syntax test_LR1_expressionParser ;  start <S> ;end grammar ;#----------------------------------------------------------------------------*