syntax galgas3DeclarationsSyntax (galgas3Scanner) {

#----------------------------------------------------------------------------------------------------------------------*

rule <declaration_with_private>
  ?let @bool inIsInternal
  ?!@galgas3DeclarationAST ioDeclarations

#----------------------------------------------------------------------------------------------------------------------*
#                                                                                                                      *
#          Start symbol                                                                                                *
#                                                                                                                      *
#----------------------------------------------------------------------------------------------------------------------*

rule <start_symbol>
 ?!@galgas3DeclarationAST ioDeclarations {
  repeat
  while
    $;$
  while
    <declaration> !?ioDeclarations
  end
}

