rule <sortedlist_sort_descriptor> ?!@sortedListSortDescriptorListAST ioSortedListSortDescriptorList {
  $identifier$ ?var  @lstring mSortedAttributeName
  @bool mAscending
  select
    $<$
    mAscending = true
  or
    $>$
    mAscending = false
  end
  ioSortedListSortDescriptorList += !mSortedAttributeName !mAscending
}

