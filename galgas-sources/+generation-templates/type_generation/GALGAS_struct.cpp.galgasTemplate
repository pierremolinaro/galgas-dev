    %const GALGAS_%![typeEntry identifierRepresentation]% & in_%![name identifierRepresentation]%,\n%!^
  end
          %Compiler * inCompiler
          %!^%COMMA_UNUSED_LOCATION_ARGS) {
  GALGAS_%!TYPE_IDENTIFIER% result ;
  result.setInitializedProperties (inCompiler) ;
%
  for (* name 3*) in CONSTRUCTOR_ARGUMENT_LIST do
    %  result.mProperty_%![name identifierRepresentation]% = in_%![name identifierRepresentation]% ;\n%
  end
%  return result ;
}

%end
