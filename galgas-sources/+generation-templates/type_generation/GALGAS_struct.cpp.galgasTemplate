  %mProperty_%![mPropertyName identifierRepresentation]% (inOperand%![IDX string]%)%
  between %,\n%
end
% {
}

//--------------------------------------------------------------------------------------------------

GALGAS_%!TYPE_IDENTIFIER% GALGAS_%!TYPE_IDENTIFIER%::constructor_new (%?^
if [CONSTRUCTOR_ARGUMENT_LIST count] == 0 then
if CONSTRUCTOR_NEEDS_COMPILER_VAR then
  %C_Compiler * inCompiler\n%
else
 %C_Compiler * /* inCompiler */\n%
end
  !^%COMMA_UNUSED_LOCATION_ARGS) {
%else
for (propertyTypeEntry propertyName 2*) in CONSTRUCTOR_ARGUMENT_LIST
  do %const GALGAS_%![propertyTypeEntry identifierRepresentation]% & in_%![propertyName string]%,\n%!^
end
if CONSTRUCTOR_NEEDS_COMPILER_VAR then
  %C_Compiler * inCompiler\n%
else
 %C_Compiler * /* inCompiler */\n%
end
  !^%COMMA_UNUSED_LOCATION_ARGS) {
%
end
!CONSTRUCTOR_INITIALIZATION_CODE
%  GALGAS_%!TYPE_IDENTIFIER% result ;
  if (%
for (* propertyName 2*) in PROPERTY_LIST
  do %in_%![propertyName string]%.isValid ()%
  between % && %
end
%) {
    result = GALGAS_%!TYPE_IDENTIFIER% (%
for (mPropertyTypeEntry propertyName 2*) in PROPERTY_LIST
  do %in_%![propertyName string]
  between %, %
end
%) ;
  }
  return result ;
}

