class GALGAS_%!TYPE_IDENTIFIER% : public %
  if SUPER_TYPE_IDENTIFIER == ""
  then %AC_GALGAS_value_class%
  else %GALGAS_%!SUPER_TYPE_IDENTIFIER
  end% {
//--------------------------------- Default constructor
  public: GALGAS_%!TYPE_IDENTIFIER% (void) ;

%if HAS_DEFAULT_CONSTRUCTOR & not IS_ABSTRACT then
%//--------------------------------- Default GALGAS constructor
  public: static GALGAS_%!TYPE_IDENTIFIER% constructor_default (LOCATION_ARGS) ;

%
end
%//--------------------------------- Embedded object pointer
  public: inline const class cPtr_%!TYPE_IDENTIFIER% * ptr (void) const {
    return (const cPtr_%!TYPE_IDENTIFIER% *) mObjectPtr ;
  }

//--------------------------------- Constructor from pointer
  public: GALGAS_%!TYPE_IDENTIFIER% (const cPtr_%!TYPE_IDENTIFIER% * inSourcePtr) ;

//--------------------------------- Property read access
%
for () in CURRENT_PROPERTY_LIST do
  %  public: class GALGAS_%![mPropertyTypeEntry identifierRepresentation]% readProperty_%![mPropertyName identifierRepresentation]% (void) const ;\n\n%
end
