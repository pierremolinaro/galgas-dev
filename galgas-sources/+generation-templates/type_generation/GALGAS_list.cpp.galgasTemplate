//--------------------------------------------------------------------------------------------------

void GALGAS_%!TYPE_IDENTIFIER%::setter_insertAtIndex (%?^
  for (propertyTypeEntry propertyName * *) in ATTRIBUTE_LIST do (IDX)
    %const GALGAS_%![propertyTypeEntry identifierRepresentation]% inOperand%![IDX string]%,\n%!^
  end
   %const GALGAS_uint inInsertionIndex,\n%!^
   %Compiler * inCompiler\n%!^
   %COMMA_LOCATION_ARGS) {
  if (isValid ()) {
    if (inInsertionIndex.isValid ()%
  for (propertyTypeEntry propertyName * *) in ATTRIBUTE_LIST
  do (IDX) % && inOperand%![IDX string]%.isValid ()%
  end
  %) {
      cCollectionElement * p = nullptr ;
      macroMyNew (p, cCollectionElement_%!TYPE_IDENTIFIER% (%
      for (propertyTypeEntry propertyName * *) in ATTRIBUTE_LIST
      do (IDX) %inOperand%![IDX string]
      between %, %
      end
      % COMMA_THERE)) ;
      capCollectionElement attributes ;
      attributes.setPointer (p) ;
      macroDetachSharedObject (p) ;
      insertObjectAtIndex (attributes, inInsertionIndex.uintValue (), inCompiler COMMA_THERE) ;
    }else{
      drop () ;
    }
  }
}

