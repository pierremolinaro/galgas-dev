    end
    %\n%!^%COMMA_LOCATION_ARGS%
  end
  %) {\n%
  %  GALGAS_%!TYPE_IDENTIFIER% result ;\n%
  if [mAssociatedValueTypeList length] == 0 then
    %  result.mEnum = kEnum_%![mConstantName identifierRepresentation]% ;\n%
