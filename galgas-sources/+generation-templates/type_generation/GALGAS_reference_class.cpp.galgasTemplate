  then %AC_GALGAS_reference_class%
  else %GALGAS_%!SUPER_TYPE_IDENTIFIER
  end% (inSourcePtr) {
  macroNullOrValidSharedObject (inSourcePtr, cPtr_%!TYPE_IDENTIFIER%) ;
}
