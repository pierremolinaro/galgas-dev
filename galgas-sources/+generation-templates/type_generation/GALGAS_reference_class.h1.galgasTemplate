class GALGAS_%!TYPE_IDENTIFIER% : public %
  if SUPER_TYPE_IDENTIFIER == ""
  then %AC_GALGAS_reference_class%
  else %GALGAS_%!SUPER_TYPE_IDENTIFIER
  end% {
//--------------------------------- Default constructor
  public: GALGAS_%!TYPE_IDENTIFIER% (void) ;

%if HAS_DEFAULT_CONSTRUCTOR & not IS_ABSTRACT then
%//--------------------------------- Default GALGAS constructor
  public: static GALGAS_%!TYPE_IDENTIFIER% constructor_default (LOCATION_ARGS) ;

%
end
%//--------------------------------- Embedded object pointer
 public: inline acStrongPtr_class * embeddedObjectPtr (void) const {
   return (acStrongPtr_class *) mObjectPtr ;
 }
  
//--------------------------------- Constructor from pointer
  public: GALGAS_%!TYPE_IDENTIFIER% (const class cPtr_%!TYPE_IDENTIFIER% * inSourcePtr) ;

