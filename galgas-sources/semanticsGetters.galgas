#———————————————————————————————————————————————————————————————————————————————————————————————————————————————————————

proc commonGetterMapForAllTypes ?!@unifiedTypeMap ioUnifiedTypeMap
                                !@getterMap outMap {
  outMap = {}
  enterInheritedGetterWithoutArgument (
   !?outMap
   !?ioUnifiedTypeMap
   !getterName:"description"
   !returnedTypeName:"string"
   !hasCompilerArgument:false
  )
  enterInheritedGetterWithoutArgument (
   !?outMap
   !?ioUnifiedTypeMap
   !getterName:"staticType"
   !returnedTypeName:"type"
   !hasCompilerArgument:false
  )
  enterInheritedGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName:"dynamicType"
    !returnedTypeName:"type"
    !hasCompilerArgument:false
  )
  enterInheritedGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName:"object"
    !returnedTypeName:"object"
    !hasCompilerArgument:false
  )
}

#———————————————————————————————————————————————————————————————————————————————————————————————————————————————————————

override method @locationPredefinedTypeAST getGetterMap
  ?!@unifiedTypeMap ioUnifiedTypeMap
  !@getterMap outMap {
  commonGetterMapForAllTypes (!?ioUnifiedTypeMap ?outMap)
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !"locationString"
    !returnedTypeName:"string"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !"locationIndex"
    !returnedTypeName:"uint"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !"file"
    !returnedTypeName:"string"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !"line"
    !returnedTypeName:"uint"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !"column"
    !returnedTypeName:"uint"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !"isNowhere"
    !returnedTypeName:"bool"
    !hasCompilerArgument:false
  )
  enterBaseGetterWithArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "union"
    !"location" !"inOtherLocation"
    !returnedTypeName:"location"
    !hasCompilerArgument: true
  )
}

