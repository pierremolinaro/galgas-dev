override method @locationPredefinedTypeAST getGetterMap
  ?!@unifiedTypeMap ioUnifiedTypeMap
  !@getterMap outMap {
  commonGetterMapForAllTypes (!?ioUnifiedTypeMap ?outMap)
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "locationString"
    !returnedTypeName:"string"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "locationIndex"
    !returnedTypeName:"uint"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "file"
    !returnedTypeName:"string"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "line"
    !returnedTypeName:"uint"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "column"
    !returnedTypeName:"uint"
    !hasCompilerArgument:true
  )
  enterBaseGetterWithoutArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "isNowhere"
    !returnedTypeName:"bool"
    !hasCompilerArgument:false
  )
  enterBaseGetterWithArgument (
    !?outMap
    !?ioUnifiedTypeMap
    !getterName: "union"
    !argSelector: "" !"location" !"inOtherLocation"
    !returnedTypeName:"location"
    !hasCompilerArgument: true
  )
}

