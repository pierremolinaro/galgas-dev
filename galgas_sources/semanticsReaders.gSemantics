  [!?outMap insertKey ![@lstring new !"ITE" !here] !t !false ![@binarysetGalgasType new]] ;
  enterReaderWithUintArgument !?outMap !"forAllOnBitIndex" ![@binarysetGalgasType new] !false ;
  enterReaderWithUintArgument !?outMap !"forAllOnBitIndexAndBeyond" ![@binarysetGalgasType new] !false ;
  enterReaderWithUintArgument !?outMap !"existOnBitIndex" ![@binarysetGalgasType new] !false ;
  enterReaderWithUintArgument !?outMap !"valueCount" ![@uint64GalgasType new] !false ;
  enterReaderWithoutArgument !?outMap !"compressedValueCount" ![@uint64GalgasType new] !false ;
  enterReaderWithoutArgument !?outMap !"uint64ValueList" !build_uint64list_type [] !false ;
  enterReaderWithoutArgument !?outMap !"stringValueList" !build_stringlist_type [] !false ;
  t := [@typeList emptyList] ;
  t += ![@uintGalgasType new] ;
  t += !build_stringlist_type [] ;
  [!?outMap insertKey ![@lstring new !"stringValueListWithNameList" !here] !t !false !build_stringlist_type []] ;
  enterReaderWithoutArgument !?outMap !"predicateStringValue" ![@stringGalgasType new] !false ;
  enterReaderWithUintArgument !?outMap !"compressedStringValueList" !build_stringlist_type [] !false ;
  enterReaderWith2UintArguments !?outMap !"swap21" ![@binarysetGalgasType new] !false ;
  enterReaderWith3UintArguments !?outMap !"swap132" ![@binarysetGalgasType new] !false ;
  enterReaderWith3UintArguments !?outMap !"swap213" ![@binarysetGalgasType new] !false ;
  enterReaderWith3UintArguments !?outMap !"swap231" ![@binarysetGalgasType new] !false ;
  enterReaderWith3UintArguments !?outMap !"swap312" ![@binarysetGalgasType new] !false ;
  enterReaderWith3UintArguments !?outMap !"swap321" ![@binarysetGalgasType new] !false ;
  enterReaderWithUintArgument !?outMap !"transitiveClosure" ![@binarysetGalgasType new] !false ;
  t := [@typeList emptyList] ;
  t += ![@binarysetGalgasType new] ;
  t += ![@uintGalgasType new] ;
  [!?outMap insertKey ![@lstring new !"accessibleStates" !here] !t !false ![@binarysetGalgasType new]] ;
  t := [@typeList emptyList] ;
  t += ![@binarysetGalgasType new] ;
  [!?outMap insertKey ![@lstring new !"equalTo" !here] !t !false ![@binarysetGalgasType new]] ;
  [!?outMap insertKey ![@lstring new !"notEqualTo" !here] !t !false ![@binarysetGalgasType new]] ;
