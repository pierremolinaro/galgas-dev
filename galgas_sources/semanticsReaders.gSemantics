once readerMapFor_binaryset ->@readerMap outMap :
  outMap := commonReaderMapForAllTypes [] ;
  enterReaderWithoutArgument !?outMap !"isFull" ![@boolGalgasType new] !false ;
  enterReaderWithoutArgument !?outMap !"isEmpty" ![@boolGalgasType new] !false ;
  enterReaderWithoutArgument !?outMap !"significantVariableCount" ![@uintGalgasType new] !false ;
  @typeList t [emptyList] ;
  t += ![@binarysetGalgasType new] ;
