:
  commonReaderMapForAllTypes !?ioUnifiedTypeMap ?outMap ;
  enterBaseReaderWithoutArgument !?outMap !?ioUnifiedTypeMap !"locationString" !"string" !true ;
  enterBaseReaderWithoutArgument !?outMap !?ioUnifiedTypeMap !"locationIndex" !"uint" !true ;
