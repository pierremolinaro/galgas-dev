once commonReaderMapForAllTypes ->@readerMap outMap :
  outMap := [@readerMap emptyMap] ;
  enterReaderWithoutArgument !?outMap !"description" ![@stringGalgasType new] !false ;
  enterReaderWithoutArgument !?outMap !"object" ![@objectGalgasType new] !false ;
  enterReaderWithoutArgument !?outMap !"dynamicType" ![@typeGalgasType new] !false ;
end once ;

