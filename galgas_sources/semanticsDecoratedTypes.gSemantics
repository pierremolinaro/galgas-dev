#---------------------------------------------------------------------------*
#                                                                           *
#    T Y P E    D E C L A R A T I O N                                       *
#                                                                           *
#---------------------------------------------------------------------------*

abstract class @semanticTypeForGeneration extends @semanticDeclarationForGeneration {
  @unifiedTypeMapProxy mTypeProxy ;
}

#---------------------------------------------------------------------------*

override reader @semanticTypeForGeneration implementationCppFileName -> @string outName
:
  outName := [mTypeProxy mHeaderFileName] ;
end reader ;

#---------------------------------------------------------------------------*

override reader @semanticTypeForGeneration hasCppHeaderFile -> @bool outHasHeader
:
  outHasHeader := true ;
end reader ;

#---------------------------------------------------------------------------*

override reader @semanticTypeForGeneration isPredefined -> @bool outResult :
  outResult := [mTypeProxy mIsPredefined] ;
end reader ;

#---------------------------------------------------------------------------*

override method @semanticTypeForGeneration addInclusionForCPPFile
  ?!@stringlist ioInclusionList
:
  for (t) in [mTypeProxy mTypesToIncludeInCppFile] do
    if ([t mHeaderFileName] != "") & not [t mIsPredefined] then
      ioInclusionList += ![t mHeaderFileName] ;
    end if ;
  end for ;
  for (s) in [mTypeProxy mSpecificHeadersForCppFile] do
    ioInclusionList += !s ;
  end for ;
end method ;

#---------------------------------------------------------------------------*

override method @semanticTypeForGeneration addHeaderInclusions
  ?!@string ioHeaderString
:
  for (t) in [mTypeProxy mTypesToIncludeInHeaderFile] do
    if not [t mIsPredefined] then
      ioHeaderString .= "#include \"" . [t mHeaderFileName]  . ".h\" // addHeaderInclusions\n" ;
    end if ;
  end for ;
end method ;

