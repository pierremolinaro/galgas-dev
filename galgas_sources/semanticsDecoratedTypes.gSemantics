end reader ;

#---------------------------------------------------------------------------*

override reader @semanticDeclarationWithHeaderForGeneration hasCppHeaderFile -> @bool outHasHeader
:
  outHasHeader := mhasHeader ;
end reader ;

#---------------------------------------------------------------------------*

override method @semanticDeclarationWithHeaderForGeneration addInclusionForCPPFile
  ?!@stringlist unused ioInclusionList
:
end method ;

#---------------------------------------------------------------------------*

override method @semanticDeclarationWithHeaderForGeneration addHeaderInclusions
  ?!@string unused ioHeaderString
:
end method ;

#---------------------------------------------------------------------------*

override reader @semanticDeclarationWithHeaderForGeneration isPredefined -> @bool outResult :
  outResult := false ;
end reader ;

