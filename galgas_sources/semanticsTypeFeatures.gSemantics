override method @stringsetPredefinedTypeAST getSuperType
  ?!@unifiedTypeMap ioUnifiedTypeMap
  !@unifiedTypeMapProxy outSuperType
:
  [@unifiedTypeMapProxy makeRegularProxy !?ioUnifiedTypeMap !"enumerable" ?outSuperType] ;
end method ;

#---------------------------------------------------------------------------*

override method @dataPredefinedTypeAST getSuperType
  ?!@unifiedTypeMap ioUnifiedTypeMap
  !@unifiedTypeMapProxy outSuperType
:
  [@unifiedTypeMapProxy makeRegularProxy !?ioUnifiedTypeMap !"enumerable" ?outSuperType] ;
end method ;

