#------------------------------------------------------------------------------*
#                                                                              *
#   E X P R E S S I O N                                                        *
#                                                                              *
#------------------------------------------------------------------------------*

rule <expression>
  ??@M_semanticsEntitiesForUse inEntitiesMap
  ??@EXsemanticContext inSemanticContext
  ??@M_optionComponents inOptionsComponentsMapForUse
  ?!@typeVariablesMap ioVariablesMap
  !@typeExpression outExpression
  !@AC_galgasType outResultType
:
  <relation_term>
    !inEntitiesMap
    !inSemanticContext
    !inOptionsComponentsMapForUse
    !?ioVariablesMap
    ?outExpression
    ?outResultType
  ;
  repeat
  while
    @location operatorLocation := here ;
    $|$ ;
    @typeExpression leftOperand := outExpression ;
    @typeExpression rightOperand ;
    @AC_galgasType firstOperandType := outResultType ;
    @AC_galgasType secondOperandType ;
    <relation_term>
      !inEntitiesMap
      !inSemanticContext
      !inOptionsComponentsMapForUse
      !?ioVariablesMap
      ?rightOperand
      ?secondOperandType
    ;
    outExpression := [@typeOrOperation new !operatorLocation !leftOperand !rightOperand] ;
    matchOrOperatorForTemplate
      !firstOperandType
      !secondOperandType
      ?outResultType
     ;
  while
    @location operatorLocation := here ;
    $^$ ;
    @typeExpression leftOperand := outExpression ;
    @typeExpression rightOperand ;
    @AC_galgasType firstOperandType := outResultType ;
    @AC_galgasType secondOperandType ;
    <relation_term>
      !inEntitiesMap
      !inSemanticContext
      !inOptionsComponentsMapForUse
      !?ioVariablesMap
      ?rightOperand
      ?secondOperandType
    ;
    outExpression := [@typeXorOperation new !operatorLocation !leftOperand !rightOperand] ;
    matchXorOperatorForTemplate
      !firstOperandType
      !secondOperandType
      ?outResultType
     ;
  end repeat ;
end rule ;

#------------------------------------------------------------------------------*
#                                                                              *
#   R E L A T I O N    T E R M                                                 *
#                                                                              *
#------------------------------------------------------------------------------*

rule <relation_term>
  ??@M_semanticsEntitiesForUse inEntitiesMap
  ??@EXsemanticContext inSemanticContext
  ??@M_optionComponents inOptionsComponentsMapForUse
  ?!@typeVariablesMap ioVariablesMap
  !@typeExpression outExpression
  !@AC_galgasType outResultType
:
  <relation_factor>
    !inEntitiesMap
    !inSemanticContext
    !inOptionsComponentsMapForUse
    !?ioVariablesMap
    ?outExpression
    ?outResultType
  ;
  repeat
  while
    @location operatorLocation := here ;
    $&$ ;
    @typeExpression leftOperand := outExpression ;
    @typeExpression rightOperand ;
    @AC_galgasType firstOperandType := outResultType ;
    @AC_galgasType secondOperandType ;
    <relation_factor>
      !inEntitiesMap
      !inSemanticContext
      !inOptionsComponentsMapForUse
      !?ioVariablesMap
      ?rightOperand
      ?secondOperandType
    ;
    outExpression := [@typeAndOperation new !operatorLocation !leftOperand !rightOperand] ;
    matchAndOperatorForTemplate
      !firstOperandType
      !secondOperandType
      ?outResultType
    ;
  end repeat ;
end rule ;

