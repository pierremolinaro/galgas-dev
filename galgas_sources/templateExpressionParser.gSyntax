routine checkNotOperatorForTemplate
  ??@AC_galgasType inOperandType
  !@AC_galgasType outResultType
:
  @typeGalgas_bool gb := (cast inOperandType if >= @typeGalgas_bool else error here) ;
  outResultType := inOperandType ;
end routine ;

