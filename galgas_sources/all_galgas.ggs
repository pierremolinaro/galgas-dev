project all_galgas:
#--- Temporary Program for PPC and i386 Cocoa GALGAS Only
  program "mda_galgas_prgm.gProgram" ;#--- Command Line Options
  option "galgas_cli_options.gOption" : makefile ;

#--- Common Scanner
  lexique "galgas_scanner.gLexique" : makefile ;

#--- Cocoa Interface
  gui       "cocoaGalgas.gGui" ;

#--- Project Creation
  semantics "projectCreation.gSemantics" : makefile ;

#--- Template
  lexique   "galgasTemplateScanner.gLexique" : makefile ;
  syntax    "templateExpressionParser.gSyntax" : makefile ;
  syntax    "galgasTemplateParser.gSyntax" : makefile ;
  grammar   "galgasTemplateGrammar.gGrammar" : makefile ;
 #--- Lexique Component
