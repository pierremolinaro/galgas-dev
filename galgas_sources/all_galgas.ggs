  semantics "predefinedLexicalActions.gSemantics" : makefile ;
  semantics "lexiqueGeneration.gSemantics" : makefile ;
  semantics "lexiqueCocoaGeneration.gSemantics" : makefile ;
  semantics "lexiqueCompilation.gSemantics" : makefile ;

#--- GUI component
