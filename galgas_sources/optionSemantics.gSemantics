semantics optionSemantics :

#---------------------------------------------------------------------------*

map @commandLineOptionMap {
  @lchar mOptionChar ;
  @lstring mOptionString ;
  @lstring mComment ;
  insert insertKey error message "the '%K' command line option has been already declared in %L" ;
