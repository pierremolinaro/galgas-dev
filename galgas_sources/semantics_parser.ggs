  @typeCplusPlusName nomCppDest ;
  [!?ioVariablesMap searchForWriteAccess !nomVariableDest ?typeVarDest ?nomCppDest] ;
  typeEntiteDestination := [@typeEntiteDest new !typeVarDest !nomCppDest !here] ;
end rule ;

