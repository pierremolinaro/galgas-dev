      @L_signature_ForGrammarComponent signatureForGrammarComponent ; # Not used here
      <formal_arguments_list>
        !?ioComponentSemanticsEntitiesMap
        ?signatureDebut
        !?tableDebut
        ?listeTypesEtNomsArgMethodeDebut
        !?tableEnAvant
        ?signatureForGrammarComponent
      ;
      $)$ ;
      $:$ ;
