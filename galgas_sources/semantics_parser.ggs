      $identifier$ ? nomMethode ;
      $:$ ;
      @L_signature signatureDebut ;
      @typeVariablesMap tableDebut [empty] ;
      @typeListeTypesEtNomsArgMethode listeTypesEtNomsArgMethodeDebut ; 
