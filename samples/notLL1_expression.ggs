#----------------------------------------------------------------------------*#                                                                            *#  This file gives an example of a grammar that is not LL(1), but SLR(1).    *#                                                                            *# From the book "COMPILATEURS, principes, techniques et outils", by Alfred   *# Aho, Ravi Sethi, Jeffrey Ullman, page 251, exemple 4.34.                   *#                                                                            *#----------------------------------------------------------------------------*#----------------------------------------------------------------------------*#  S C A N N E R                                                             *#----------------------------------------------------------------------------*lexique myNotLL1_scanner :#--- Identifiers$id$ error message "an identifier" ;rule 'a'->'z' | 'A'->'Z' :  send $id$ ;end rule ;#--- Delimitorslist delimitorsList error message "the '%' delimitor" :  "+" , "*", "(", ")" ;rule list delimitorsList ;# --------------------- separators -----------------------------------------rule '\1' -> ' ' :end rule ;end lexique ;#----------------------------------------------------------------------------*#  P A R S E R                                                               *#----------------------------------------------------------------------------*syntax myNotLL1_expressionParser :import lexique myNotLL1_scanner ;rule <E> ;rule <T> ;rule <F> ;rule <E> :  <T> ;end rule ;rule <E> :  <E> ;  $+$ ;  <T> ;end rule ;rule <T> :  <F> ;end rule ;rule <T> :  <T> ;  $*$ ;  <F> ;end rule ;rule <F> :  $id$ ;end rule ;rule <F> :  $($ ; <E> ; $)$ ;end rule ;end syntax ;#----------------------------------------------------------------------------*#  G R A M M A R                                                             *#----------------------------------------------------------------------------*grammar myNotLL1_grammar "" :  import lexique myNotLL1_scanner ;  import syntax myNotLL1_expressionParser ;  start <E> ;end grammar ;#--------------------------------------------------------------------------------*