#----------------------------------------------------------------------------*#                                                                            *#  This file gives an example of a grammar that is not SLR(1), but LR(1).    *#                                                                            *# From the book "COMPILATEURS, principes, techniques et outils", by Alfred   *# Aho, Ravi Sethi, Jeffrey Ullman, page 257, exemple 4.39.                   *#                                                                            *#----------------------------------------------------------------------------*#----------------------------------------------------------------------------*#  S C A N N E R                                                             *#----------------------------------------------------------------------------*lexique myNotSLR1_scanner :#--- Identifiers$id$ error message "an identifier" ;rule 'a'->'z' | 'A'->'Z' :  send $id$ ;end rule ;#--- Delimitorslist delimitorsList error message "the '%' delimitor" :  "=" ,    "*"   ;rule list delimitorsList ;# --------------------- separatorsrule '\1' -> ' ' :end rule ;end lexique ;#----------------------------------------------------------------------------*#  P A R S E R                                                               *#----------------------------------------------------------------------------*syntax myNotSLR1_expressionParser :import lexique myNotSLR1_scanner ;rule <S> ;rule <G> ;rule <D> ;rule <S> : <G> ; $=$ ; <D> ;end rule ;rule <S> : <D> ;end rule ;rule <G> : $*$ ; <D> ;end rule ;rule <G> : $id$ ;end rule ;rule <D> : <G> ;end rule ;end syntax ;#----------------------------------------------------------------------------*#  G R A M M A R                                                             *#----------------------------------------------------------------------------*grammar myNotSLR1_grammar "" :  import lexique myNotSLR1_scanner ;  import syntax myNotSLR1_expressionParser ;  start <S> ;end grammar ;#----------------------------------------------------------------------------*